      "edgeToEdgeEnabled": true,
      "package": "com.anand1765.frontend"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "42cbe211-4401-4389-be72-d224eb6ba5c0"
      }
    }
